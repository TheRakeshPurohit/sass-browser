{
  "name": "sass-browser",
  "version": "0.1.2",
  "description": "A tiny wrapper around sass.js to allow to use like less.js on browser",
  "main": "sass-browser.js",
  "files": [
    "sass-browser.js",
    "dist/sass-browser.min.js"
  ],
  "scripts": {
    "start": "npm run open",
    "open": "concurrently \"http-server -a localhost -p 1234\" \"opener http://localhost:1234/\"",
    "build": "terser sass-browser.js -o dist/sass-browser.min.js"
  },
  "author": "Luis Panadero Guarde√±o  <luis(dot)panadero(at)gmail.com>",
  "license": "MIT",
  "keywords": [
    "sass",
    "scss",
    "css",
    "browser"
  ],
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "http-server": "^0.12.3",
    "opener": "^1.5.1",
    "prettier": "^1.19.1",
    "terser": "^4.8.0"
  },
  "dependencies": {
    "sass.js": "^0.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zardoz89/sass-browser.git"
  },
  "prettier": {
    "printWidth": 120,
    "endOfLine": "auto"
  }
}
